@model IEnumerable<RentaAutos.Models.RENTA>

@{
    ViewBag.Title = "Renta";
}

<div class="panel-heading">
    <h3>@ViewBag.Title  | @Html.ActionLink(" ", "Create", null, new { @class = "btn-default glyphicon-plus", data_toggle = "tooltip", data_placement = "bottom", data_original_title = "Nuevo" }) </h3>
</div>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AUTOMOVIL.GAMA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USUARIO.USUARIO1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PRECIO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FECHA_ALQUILER)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AUTOMOVIL.GAMA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USUARIO.USUARIO1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRECIO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHA_ALQUILER)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_RENTA }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_RENTA }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_RENTA })
        </td>
    </tr>
}

</table>
